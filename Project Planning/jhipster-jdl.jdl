entity Stats {
distanceRan Float
time Duration
cadence Integer max(200)
avgpace Float
}

entity Trends{
avgPace Float
distancePerRun Float
}

entity Ranking {
name String 
avgpace Float
rank Integer
}

entity Map {
distance Float
timeStart Instant
timeStop Instant
}

relationship OneToOne {
Trends{user(login)} to User
Stats{user(login)} to User
Ranking{user(login)} to User
Map{stats(distance)} to Stats
}

relationship OneToMany {
Ranking{stats(avgpace)} to Stats
  
  
}


    